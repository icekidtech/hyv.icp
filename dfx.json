{
  "canisters": {
    "hyv_backend": {
      "main": "src/hyv_backend/main.mo",
      "type": "motoko"
    },
    "hyv_generator": {
      "type": "motoko",
      "main": "src/hyv_generator/main.mo"
    },
    "hyv_frontend": {
      "dependencies": [
        "hyv_backend",
        "hyv_generator"
      ],
      "frontend": {
        "entrypoint": "src/hyv_frontend/src/index.html"
      },
      "source": [
        "src/hyv_frontend/dist"
      ],
      "type": "assets",
      "workspace": "hyv_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    }
  }
}
